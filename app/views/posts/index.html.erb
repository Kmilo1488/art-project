<h1>Post</h1>

  <table class="table">
      <thead>
        <tr>
          <th scope="col">Title</th>
          <th scope="col">View</th>
        </tr>
      </thead>
      <%- @posts.each do |post| %>
        <tbody>
          <tr>
            <td> <%= post.title %> </td>
            <td> <%= link_to "more", post_path(post) %> </td>
          </tr>
        </tbody>
      <% end -%>
  </table>
