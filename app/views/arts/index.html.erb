<h1>Mucha Arte</h1>


  <table class="table">
      <thead>
        <tr>
          <th scope="col">Title</th>
          <th scope="col">View</th>
        </tr>
      </thead>
      <%- @arts.each do |art| %>
        <tbody>
          <tr>
              <td> <%= art.title %> </td>
              <td> <%= link_to "more", art_path(art) %> </td>
          </tr>
        </tbody>
      <% end -%>
  </table>
